		^slot2 <rtans>)
	(<n> ^slot1 nil
		^slot2 nil
		^slot3 <w>
		^slot4 nil)
	(<a> ^ACout1 <a1> -
		^ACout2 <a2> -
		^ACout1 |read|
		^ACout2 |algae|)
	#(write |# Lesser-evil: read-algae| (crlf))
}

